pipeline{
    agent any
    triggers {
        pollSCM '* * * * *'
    }
    tools{ nodejs "NodeJS"}
    stages{
        stage("install dependencies"){
            steps{
               sh "npm install"
            }
        }
    }
    stages{
        stage("npm build"){
            steps{
               sh "npm build"
            }
        }
    }
    
}